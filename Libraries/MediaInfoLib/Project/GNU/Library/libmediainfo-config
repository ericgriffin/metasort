#!/bin/bash

if test "$1" = "CXXFLAGS"; then
echo  -DUNICODE
fi
if test "$1" = "LIBS"; then
echo  -L/home/eric/Desktop/metasort/Mediainfo/MediaInfoLib/Project/GNU/Library -lmediainfo  -lz -L/home/eric/Desktop/metasort/Mediainfo/ZenLib/Project/GNU/Library -lzen -lpthread -lstdc++
fi
if test "$1" = "LIBS_Static"; then
echo  /home/eric/Desktop/metasort/Mediainfo/MediaInfoLib/Project/GNU/Library/.libs/libmediainfo.a  -lz -L/home/eric/Desktop/metasort/Mediainfo/ZenLib/Project/GNU/Library -lzen -lpthread -lstdc++
fi
if test "$1" = "Unicode"; then
echo yes
fi
if test "$1" = "Exists"; then
echo yes
fi
if test "$1" = "la_name"; then
echo libmediainfo.la
fi
